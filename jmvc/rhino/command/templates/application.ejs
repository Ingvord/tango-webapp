include.namespace('<%= namespace %>');
include.css(
    <%= print_resources(stylesheets) %>
);
include.resources(
    <%= print_resources(resources) %>
);
include.engines(
    <%= print_resources(engines) %>
);
include.plugins(
    <%= print_resources(plugins) %>
    );

include(function(){ //runs after prior includes are loaded
  include.models(
        <%= print_resources(models) %>
    );
  include.controllers(
        <%= print_resources(controllers) %>
    );
  include.views(
        <%= print_resources(views) %>
    );
});